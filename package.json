{
	"name": "minification-benchmarks",
	"private": true,
	"description": "JS minification benchmarks",
	"license": "MIT",
	"repository": "privatenumber/minification-benchmarks",
	"author": "Hiroki Osame <hiroki.osame@gmail.com>",
	"files": [],
	"scripts": {
		"prepare": "simple-git-hooks",
		"benchmark": "tsx scripts/benchmark",
		"benchmark-all": "tsx scripts/benchmark-all.ts",
		"update-benchmarks-readme": "tsx scripts/update-benchmarks-readme/index.ts",
		"lint": "eslint --cache .",
		"typecheck": "tsc --noEmit",
		"postinstall": "bina tdewolff/minify --install-dir node_modules/.bin"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache"
	},
	"dependencies": {
		"@swc/core": "^1.3.21",
		"antd": "^4.16.1",
		"babel-minify": "^0.5.2",
		"byte-size": "^8.1.0",
		"cleye": "^1.2.1",
		"comment-mark": "^1.1.1",
		"d3": "^6.3.1",
		"date-fns": "^2.27.0",
		"echarts": "^5.1.1",
		"esbuild": "^0.16.3",
		"execa": "^6.1.0",
		"fs-require": "^1.4.0",
		"google-closure-compiler": "^20221102.0.1",
		"jquery": "^3.5.1",
		"jsdom": "^20.0.0",
		"jsdom-global": "^3.0.2",
		"lodash": "^4.17.21",
		"make-dir": "^3.1.0",
		"markdown-table": "^3.0.2",
		"mem": "^9.0.2",
		"memfs": "^3.4.7",
		"moment": "^2.29.1",
		"outdent": "^0.8.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"read-pkg-up": "^9.1.0",
		"tasuku": "^2.0.0",
		"terser": "^5.16.1",
		"three": "^0.124.0",
		"typescript": "^4.8.3",
		"uglify-js": "^3.17.4",
		"victory": "^35.8.4",
		"vue": "^2.6.12"
	},
	"devDependencies": {
		"@pvtnbr/eslint-config": "^0.30.1",
		"@types/byte-size": "^8.1.0",
		"@types/google-closure-compiler": "^0.0.19",
		"@types/jsdom-global": "^3.0.3",
		"@types/lodash": "^4.14.178",
		"@types/minimist": "^1.2.2",
		"@types/node": "^16.11.33",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/uglify-js": "^3.17.0",
		"bina": "^0.1.3",
		"eslint": "^8.24.0",
		"lint-staged": "^13.0.3",
		"simple-git-hooks": "^2.8.0",
		"tsx": "^3.1.0"
	},
	"eslintConfig": {
		"extends": "@pvtnbr",
		"ignorePatterns": [
			"results"
		]
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"dayjs"
			]
		}
	}
}

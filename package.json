{
	"name": "minification-benchmarks",
	"description": "JS minification benchmarks",
	"license": "MIT",
	"repository": "privatenumber/minification-benchmarks",
	"author": "Hiroki Osame <hiroki.osame@gmail.com>",
	"private": true,
	"files": [],
	"packageManager": "pnpm@10.17.0",
	"scripts": {
		"bench": "tsx packages/bench/benchmark/cli.ts",
		"bench-all": "tsx packages/bench/benchmark-all/cli.ts",
		"update-readme": "tsx packages/data/update-readme/index.ts",
		"lint": "lintroll --cache . --ignore-pattern ./packages/data/data/data.json",
		"type-check": "tsc"
	},
	"devDependencies": {
		"@minification-benchmarks/artifacts": "workspace:*",
		"@minification-benchmarks/minifiers": "workspace:*",
		"@minification-benchmarks/utils": "workspace:*",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.19.25",
		"lintroll": "^1.25.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"dayjs"
			]
		},
		"onlyBuiltDependencies": [
			"bun"
		]
	}
}

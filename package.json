{
	"name": "minification-benchmarks",
	"description": "JS minification benchmarks",
	"license": "MIT",
	"repository": "privatenumber/minification-benchmarks",
	"author": "Hiroki Osame <hiroki.osame@gmail.com>",
	"private": true,
	"files": [],
	"scripts": {
		"benchmark": "esno scripts/benchmark",
		"benchmark-all": "esno scripts/benchmark-all",
		"update-benchmarks-readme": "esno scripts/update-benchmarks-readme/index.ts",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"postinstall": "bina tdewolff/minify --install-dir node_modules/.bin"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,js}": "eslint"
	},
	"dependencies": {
		"@swc/core": "^1.2.143",
		"antd": "^4.16.1",
		"babel-minify": "^0.5.1",
		"byte-size": "^8.1.0",
		"cleye": "^1.0.1",
		"comment-mark": "^1.1.1",
		"d3": "^6.3.1",
		"date-fns": "^2.27.0",
		"echarts": "^5.1.1",
		"esbuild": "^0.14.23",
		"execa": "^6.0.0",
		"fs-require": "^1.4.0",
		"google-closure-compiler": "^20220202.0.0",
		"jquery": "^3.5.1",
		"jsdom": "^19.0.0",
		"jsdom-global": "^3.0.2",
		"lodash": "^4.17.21",
		"make-dir": "^3.1.0",
		"markdown-table": "^3.0.1",
		"mem": "^9.0.1",
		"memfs": "^3.4.0",
		"moment": "^2.29.1",
		"outdent": "^0.8.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.2",
		"read-pkg-up": "^9.0.0",
		"tasuku": "^1.0.2",
		"terser": "^5.10.0",
		"three": "^0.124.0",
		"uglify-js": "^3.15.1",
		"victory": "^35.8.4",
		"vue": "^2.6.12"
	},
	"devDependencies": {
		"@pvtnbr/eslint-config": "^0.7.1",
		"@types/byte-size": "^8.1.0",
		"@types/lodash": "^4.14.178",
		"@types/minimist": "^1.2.2",
		"@types/node": "^16.11.17",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"bina": "^0.1.3",
		"eslint": "^8.3.0",
		"esno": "^0.12.1",
		"husky": "^4.3.8",
		"lint-staged": "^12.1.2",
		"typescript": "^4.5.2"
	},
	"eslintConfig": {
		"extends": "@pvtnbr",
		"ignorePatterns": [
			"results"
		],
		"rules": {
			"@typescript-eslint/no-shadow": [
				"error",
				{
					"allow": [
						"task"
					]
				}
			]
		}
	}
}

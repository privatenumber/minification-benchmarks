{
	"name": "minification-benchmarks",
	"private": true,
	"description": "JS minification benchmarks",
	"license": "MIT",
	"repository": "privatenumber/minification-benchmarks",
	"author": "Hiroki Osame <hiroki.osame@gmail.com>",
	"files": [],
	"packageManager": "pnpm@8.5.1",
	"scripts": {
		"prepare": "simple-git-hooks",
		"benchmark": "tsx scripts/benchmark",
		"benchmark-all": "tsx scripts/benchmark-all.ts",
		"update-benchmarks-readme": "tsx scripts/update-benchmarks-readme/index.ts",
		"lint": "eslint --cache .",
		"type-check": "tsc --noEmit"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*.{ts,js}": "eslint --cache"
	},
	"dependencies": {
		"byte-size": "^8.1.0",
		"cleye": "^1.3.1",
		"comment-mark": "^1.1.1",
		"date-fns": "^2.27.0",
		"execa": "^6.1.0",
		"fs-require": "^1.6.0",
		"make-dir": "^3.1.0",
		"markdown-table": "^3.0.2",
		"memfs": "^3.4.13",
		"outdent": "^0.8.0",
		"tasuku": "^2.0.1",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@minification-benchmarks/artifacts": "*",
		"@minification-benchmarks/minifiers": "*",
		"@minification-benchmarks/utils": "*",
		"@pvtnbr/eslint-config": "^0.37.1",
		"@types/byte-size": "^8.1.0",
		"@types/lodash": "^4.14.178",
		"@types/minimist": "^1.2.2",
		"@types/node": "^16.11.33",
		"@types/react": "^17.0.38",
		"eslint": "^8.50.0",
		"lint-staged": "^15.2.0",
		"simple-git-hooks": "^2.9.0",
		"tsx": "^4.7.0",
		"typescript": "^5.2.2"
	},
	"eslintConfig": {
		"extends": "@pvtnbr",
		"ignorePatterns": [
			"results"
		]
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"dayjs"
			]
		}
	}
}
